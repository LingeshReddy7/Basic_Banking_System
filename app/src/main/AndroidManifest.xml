<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.basic_banking_system">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".aboutMe"
            android:label="About Me"
            android:parentActivityName=".MainActivity"></activity>
        <activity
            android:name=".My_account"
            android:label="My Account"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".transaction_activity"
            android:label="All Transactions"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".moneySendActivity"
            android:label="Safe Transation"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".userData"
            android:label="User Details"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Add_User"
            android:label="Add New User"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".MainActivity"
            android:label="Bank System">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>